//Matrix multiplication
#include <stdio.h>
int main() 
{
    int array1[2][2] = {{1, 2}, {3, 4}};
    int array2[2][2] = {{5, 6}, {7, 8}};
    int rows1 = 2;
    int cols1 = 2;
    int cols2 = 2;
    int result[2][2];
    for (int i = 0; i < rows1; ++i) {
        for (int j = 0; j < cols2; ++j) {
            int sum = 0;
            for (int k = 0; k < cols1; ++k) {
                sum += array1[i][k] * array2[k][j];
            }
            result[i][j] = sum;
        }
    }

    for (int i = 0; i < rows1; ++i) {
        for (int j = 0; j < cols2; ++j) {
            printf("%d ", result[i][j]);
        }
        printf("\n");
    }

    return 0;
}



Output:
19 22 
43 50 


