#include <stdio.h>
int main() 
{
int score;
printf("Enter your Score:");
scanf("%d",&score);
if (score >= 80)
	printf("Your grade is A+");
else if (score >= 75)
	printf("Your grade is A");
else if (score >= 70)
	printf("Your grade is A-");
else if (score >= 65)
	printf("Your grade is B+");
else if (score >= 60)
	printf("Your grade is B");
else if (score >= 55)
	printf("Your grade is B-");
else if (score >= 50)
	printf("Your grade is C+");
else if (score >= 45)
	printf("Your grade is C");
else if (score >= 40)
	printf("Your grade is D");
else
	printf("Your grade is F");
return 0;
}

Output:
Enter your Score:75
Your grade is A
