//Using nested if
# include <stdio.h>
	int main ()
	{
	int Age;
	printf ("Please enter your age : ");
	scanf ("%d", &Age);
	if (Age<= 0)
	printf ("\n You have not borned yet!");
	if (Age > 0 && Age <= 12)
	printf (" \n You are a child.");
	if (Age > 12 && Age <= 19)
	printf (" \n You are a teenager.");
	if (Age > 19 && Age <= 40)
	printf (" \n You are young.");
	if (Age>40)
	printf (" \nWish your long life.");
	return 0;
	}


Output:
