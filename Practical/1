#include <stdio.h>
	int main ( )
	{
		printf ("Haque Publications");
		printf ("\n Bangla Bazar, Dhaka-1100");
		return 0;
	}


Output:
Haque Publications
 Bangla Bazar, Dhaka-1100

 //1.2
 #include<stdio.h>
int main()
{
     int num1,num2,sum;
     printf("Enter the value of num1:");
     scanf("%d",&num1);
     printf("Enter the value of num2:");
     scanf("%d",&num2);
     sum=num1+num2;
     printf("The Sumation of two number is:%d",sum);
     return 0;
}


Output:
Enter the value of num1:10
Enter the value of num2:12
The Sumation of two number is:22


