#include <stdio.h>
#include <string.h>
#include <ctype.h>

int main() 
{
    char string[] = "Mohammed Mahbub Alam";
    int length = strlen(string);
    
    for (int i = 0; i < length; i++) {
        string[i] = toupper(string[i]);
    }
    
    printf("%s\n", string);
    return 0;
}



Output:
MOHAMMED MAHBUB ALAM
