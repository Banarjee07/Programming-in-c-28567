#include<stdio.h>
long int factorial(int n);
void main()
{
int n;
printf("Enter the value of n:");
scanf("%d",&n);
printf("Factorial of %d is=%d",n,factorial(n));
}
long int factorial(int n)
{if(n<=1)
return(1);
else
return(n*factorial(n-1));
}


